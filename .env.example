# (Required) Path to your headers_auth.json file exported from YouTube Music.
HEADERS_RAW_FILE=./headers_raw.txt

# (Required) Path to your browser.json file generated by the YTM Desktop app.
AUTH_GENERATED_FILE=./browser.json

# (Required) Delay in seconds between YouTube API requests to avoid hitting rate limits.
YT_API_DELAY_SECONDS=1

# (Required) Maximum number of retries for YouTube API requests in case of failure.
YT_API_MAX_RETRIES=3

# (Optional) Directory where logs will be stored.
LOG_DIR=./logs

# (Optional) Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL). Default: INFO
LOG_LEVEL=INFO
